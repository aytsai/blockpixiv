{
	"name": "Pixiv Art Block",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Block art you don't want to see while browsing on Pixiv. あなたが見たくないイラストをブロック.",
	"icons": { "16" : "icon.png",
			   "48" : "icon.png",
               "128" : "icon.png"},
	"content_scripts": [{
		"all_frames": false,
		"matches": ["<all_urls>"],
		"exclude_matches": [],
			"js": ["jquery-1.11.1.min.js"],
			"css": []
    }],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	}
}